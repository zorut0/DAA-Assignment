def find_max_element(nums):
    if not nums:
        return None
    
    max_value = float('-inf')  # Initialize with negative infinity
    
    for num in nums:
        if num > max_value:
            max_value = num
    
    return max_value

print(find_max_element([1, 2, 3, 4, 5]))    # Output: 5
print(find_max_element([7, 7, 7, 7, 7]))    # Output: 7
print(find_max_element([-10, 2, 3, -4, 5])) # Output: 5
